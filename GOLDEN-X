FROM quay.io/https:/GOLDEN-MEGA-MD-V1
RUN git clone https://https://github.com/megaking789/Golden-mega-md-v1-whatsapp-bot /root/smdd
RUN rm -rf /root/smdd/.git
WORKDIR /root/smdd
RUN npm install || yarn install
EXPOSE 8000
CMD ["npm","start" ] 
































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































